{
  "name": "azle_chain_fusion_starter",
  "build": { "dockerfile": "Dockerfile" },
  "forwardPorts": [4943],
  "onCreateCommand": "nohup bash -c 'npm install'",
  "portsAttributes": {
    "4943": {
      "label": "dfx",
      "onAutoForward": "ignore"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": ["dfinity-foundation.vscode-motoko", "esbenp.prettier-vscode", "juanblanco.solidity"]
    },
    "codespaces": {
      "openFiles": ["README.md"]
    }
  }
}
